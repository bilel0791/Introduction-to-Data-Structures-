//procedure  calcule variable ps
PROCEDURE dot_product(v1, v2)
VAR
ps:INTEGER:=0
BEGIN
FOR i FROM 0 TO v1.lenght-1  DO
ps := ps + (v1[i] * v2[i]);
END_FOR
IF (ps=0) THEN
        write( "Les vecteurs sont orthogonaux");
    ELSE
        write("Les vecteurs ne sont pas orthogonaux");
    END_IF
End
ALGORITHM Problem2
VAR
//declaration de vecteur
    v1,v2:ARRAY_OF INTEGER []
BEGIN
    Read(v1);
    Read(v2);
    //appel de procedure
    dot_product(v1,v2);
END
